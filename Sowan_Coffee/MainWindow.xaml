<Window x:Class="Sowan_Coffee.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sowan_Coffee"
        mc:Ignorable="d"
        d:DesignHeight="844" d:DesignWidth="390"
        Title="Sowan Coffee" Height="844" Width="390" MaxWidth="390" MaxHeight="844" MinWidth="390" MinHeight="844" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
    <Window.Background>
        <ImageBrush ImageSource="Scan Barcode.png"/>
    </Window.Background>
    <StackPanel Width="390" MaxWidth="390" MinWidth="390">
        <DockPanel>
            <Frame x:Name="NavigationScan" NavigationUIVisibility="Hidden" />
        </DockPanel>
        <Button Content="Scan" HorizontalAlignment="Left" Height="51" Margin="109,617,0,0" VerticalAlignment="Top" Width="173" Background="#FFD9D9D9" FontFamily="Poppins" FontSize="20" FontWeight="Bold" Click="BtnClick">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="25"/>
                </Style>
            </Button.Resources>
        </Button>

    </StackPanel>
</Window>
