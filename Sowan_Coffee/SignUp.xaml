<Page x:Class="Sowan_Coffee.SignUp"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sowan_Coffee"
      mc:Ignorable="d" 
      d:DesignHeight="844" d:DesignWidth="390"
      Title="SignUp" Height="844" Width="390" MaxWidth="390" MaxHeight="844" MinWidth="390" MinHeight="844" Background="#FF0C0F14" >

    <StackPanel>
        <DockPanel>
            <Frame x:Name="NavigationSignUp" NavigationUIVisibility="Hidden" />
        </DockPanel>
        <Image Source="/img/Logo2.png" Width="236" Height="53" Margin="0,170,0,0" HorizontalAlignment="Center"></Image>
        <TextBlock Text="Sign Up" Foreground="White" FontFamily="Poppins" FontSize="20" FontWeight="Medium" Width="76" Margin="68,50,0,0" Padding="0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Grid  Width="270" Height="59">
            <TextBox Height="49" Background="Transparent" Foreground="#FFBEBEBE" Width="270" Margin="0,10,0,0">
                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBlock Margin="29,24,197,15" Height="21" Padding="0" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Text="Nama" Foreground="#FF83868B"/>
        </Grid>

        <Grid  Width="270" Height="59">
            <TextBox Height="49" Background="Transparent" Foreground="#FFBEBEBE" Width="270" Margin="0,10,0,0">
                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBlock Margin="29,24,160,14" Height="21" Padding="0" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Text="Nomer Telp" Foreground="#FF83868B"/>
        </Grid>
        <Grid  Width="270" Height="59">
            <TextBox Height="49" Background="Transparent" Foreground="#FFBEBEBE" Width="270" Margin="0,10,0,0">
                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBlock Margin="29,24,202,14" Height="21" Padding="0" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Text="Email" Foreground="#FF83868B"/>
        </Grid>
        <Grid  Width="270" Height="59">
            <TextBox Height="49" Background="Transparent" Foreground="#FFBEBEBE" Width="270" Margin="0,10,0,0">
                <TextBox.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </TextBox.Resources>
            </TextBox>
            <TextBlock Margin="29,24,171,15" Height="21" Padding="0" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Text="Password" Foreground="#FF83868B"/>
        </Grid>
        <Button Content="Sign Up" Width="270" Height="49" Margin="0,29,0,0" Foreground="White" BorderBrush="#00707070" Background="#FF693D21" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Click="BtnClick5">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="15"/>
                </Style>
            </Button.Resources>
        </Button>
        <Grid Height="21" Margin="0,30,0,0" Width="184">
            <TextBlock Height="21" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Text="have an account?" Foreground="Gray" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Button Height="24" FontFamily="Poppins" FontSize="14" FontWeight="Medium" Foreground="#FF693D21" Margin="0,-2,-1,-1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Content="Sign In" BorderBrush="#00707070" Background="#00DDDDDD" Width="57" Click="BtnClick6"/>

        </Grid>
    </StackPanel>
</Page>
